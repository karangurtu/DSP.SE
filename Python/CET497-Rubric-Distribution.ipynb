    "import matplotlib.pyplot as plt\n",
    "from numpy import convolve, arange, linspace, sum\n",
    "# x = [1/3,1/3,1/3]\n",
    "x = [0.0,0.4,0.6]\n",
    "y = [1]\n",
    "N = 21\n",
    "for count in arange(N):\n",
    "    y = convolve(y,x)\n",
    "percent = linspace(0,100,len(y))\n",
    "plt.plot(percent,y)\n",
    "print(sum(y * percent))\n",
    "\n"
   ]
